#!/bin/bash

set -e

case "$1" in
  configure)
      /usr/share/mysql-common/configure-symlinks install mariadb "/etc/mysql/mariadb.cnf"
  ;;

  abort-upgrade|abort-remove|abort-configure)
  ;;

  *)
    echo "postinst called with unknown argument '$1'" 1>&2
    exit 1
  ;;
esac

#DEBHELPER#

exit 0
